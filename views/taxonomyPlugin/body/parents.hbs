<div style='height:100%;margin-bottom: 15px;'>
    <ul style='list-style-type: none; padding-left: 5px;'>
        {{#each parents}}
            <li data-module="{{module}}" data-concept-id='{{conceptId}}' data-term='{{defaultTerm}}' data-descendants='{{descendantCount}}' class='treeLabel'>
                <button class='btn btn-link btn-xs treeButton' style='padding:2px'>
                    <i class='glyphicon glyphicon-chevron-{{#if_def conceptId}}down{{else}}up{{/if_def}} treeButton' id='{{../divElementId}}-treeicon-{{conceptId}}'></i>
                </button>
                {{#if_eq definitionStatus "PRIMITIVE"}}
                    <span class="badge alert-warning" data-definition-status="{{definitionStatus}}" data-module="{{module}}" data-concept-id="{{conceptId}}" data-term="{{defaultTerm}}" draggable="true" ondragstart="drag(event, '{{divElementId}}')">&nbsp;&nbsp;</span>&nbsp;&nbsp;
                {{else}}
                    <span class="badge alert-warning" data-definition-status="{{definitionStatus}}" data-module="{{module}}" data-concept-id="{{conceptId}}" data-term="{{defaultTerm}}" draggable="true" ondragstart="drag(event, '{{divElementId}}')">&equiv;</span>&nbsp;&nbsp;
                {{/if_eq}}
                {{#hasCountryIcon module}}
                    <div class="phoca-flagbox" style="width:33px;height:33px">
                        <span class="phoca-flag {{countryIcon module}}"></span>
                    </div>
                {{/hasCountryIcon}}
                <a href="javascript:void(0);" style="color: inherit;text-decoration: inherit;">
                    <span data-module="{{module}}" draggable="true" ondragstart="drag(event, '{{../../divElementId}}')" data-definition-status="{{definitionStatus}}" data-concept-id="{{conceptId}}" data-term="{{defaultTerm}}" data-descendants="{{descendantCount}}" class="treeLabel selectable-row" id="{{../divElementId}}-treenode-{{conceptId}}">{{defaultTerm}}</span>
                </a>
        {{/each}}
        {{#if_gr parents.length 0}}
            {{slice 0 -5}}
        {{/if_gr}}
        <ul style='list-style-type: none; padding-left: 15px;'>
            <li data-definition-status="{{focusConcept.definitionStatus}}" data-module="{{focusConcept.module}}" data-concept-id='{{focusConcept.conceptId}}' data-term='{{focusConcept.defaultTerm}}' data-statedDescendants='{{focusConcept.statedDescendants}}' class='treeLabel'>
                <button class='btn btn-link btn-xs treeButton' style='padding:2px'><i class='glyphicon glyphicon-chevron-right treeButton'  id='{{divElementId}}-treeicon-{{focusConcept.conceptId}}'></i></button>
                {{#if_eq focusConcept.definitionStatus "PRIMITIVE"}}
                    <span class="badge alert-warning" draggable="true" ondragstart="drag(event, '{{divElementId}}')" data-definition-status="{{focusConcept.definitionStatus}}" data-module="{{focusConcept.module}}" data-concept-id="{{focusConcept.conceptId}}" data-term="{{focusConcept.defaultTerm}}">&nbsp;&nbsp;</span>&nbsp;&nbsp;
                {{else}}
                    <span class="badge alert-warning" draggable="true" ondragstart="drag(event, '{{divElementId}}')" data-definition-status="{{focusConcept.definitionStatus}}" data-module="{{focusConcept.module}}" data-concept-id="{{focusConcept.conceptId}}" data-term="{{focusConcept.defaultTerm}}">&equiv;</span>&nbsp;&nbsp;
                {{/if_eq}}
                {{#hasCountryIcon focusConcept.module}}
                    <div class="phoca-flagbox" style="width:33px;height:33px">
                        <span class="phoca-flag {{countryIcon focusConcept.module}}"></span>
                    </div>
                {{/hasCountryIcon}}
                <a href="javascript:void(0);" style="color: inherit;text-decoration: inherit;">
                    <span draggable="true" ondragstart="drag(event, '{{../../divElementId}}')" data-definition-status="{{focusConcept.definitionStatus}}" data-module="{{focusConcept.module}}" data-concept-id="{{focusConcept.conceptId}}" data-term="{{focusConcept.defaultTerm}}" data-descendants="{{focusConcept.descendantCount}}" class="treeLabel selectable-row" id="{{divElementId}}-treenode-{{focusConcept.conceptId}}">{{focusConcept.defaultTerm}}</span>
                </a>
            </li>
        </ul>
        {{#if_gr parents.length 0}}
            </li>
        {{/if_gr}}
    </ul>
</div>