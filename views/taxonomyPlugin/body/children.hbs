<ul style='list-style-type: none; padding-left: 15px;'>
    {{#each result}}
        {{#if active}}
            <li data-module="{{module}}" data-concept-id='{{conceptId}}' data-term='{{fsn}}' class='treeLabel'>
                <button class='btn btn-link btn-xs treeButton' style='padding:2px'>
                    <i class='glyphicon glyphicon-{{#if_eq hasChild false}}minus{{else}}chevron-right{{/if_eq}} treeButton' id='{{../../divElementId}}-treeicon-{{conceptId}}'></i>
                </button>
                {{#if_eq definitionStatus "Primitive"}}
                    <span class="badge alert-warning" draggable="true" ondragstart="drag(event, '{{../../divElementId}}')" data-module="{{module}}" data-concept-id="{{conceptId}}" data-term="{{fsn}}">&nbsp;&nbsp;</span>&nbsp;&nbsp;
                {{else}}
                    <span class="badge alert-warning" draggable="true" ondragstart="drag(event, '{{../../divElementId}}')" data-module="{{module}}" data-concept-id="{{conceptId}}" data-term="{{fsn}}">&equiv;</span>&nbsp;&nbsp;
                {{/if_eq}}
                {{#hasCountryIcon module}}
                    <div class="phoca-flagbox" style="width:26px;height:26px">
                        <span class="phoca-flag {{countryIcon module}}"></span>
                    </div>
                {{/hasCountryIcon}}
                <a href="javascript:void(0);" style="color: inherit;text-decoration: inherit;">
                    <span class="treeLabel selectable-row" data-module="{{module}}" data-concept-id="{{conceptId}}" data-term="{{fsn}}" id="{{../../divElementId}}-treenode-{{conceptId}}">{{fsn}}</span>
                </a>
                {{push conceptId}}
            </li>
        {{/if}}
    {{/each}}
</ul>
