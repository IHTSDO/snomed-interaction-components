{{#if_eq options.selectedView "stated"}}
    {{#if_eq statedParents.length 0}}
        <span class='text-muted'>No parents</span>
    {{else}}
        <ul style='list-style-type: none; padding-left: 2px;'>
            {{#each statedParents}}
                <li data-concept-id='{{target.conceptId}}'>
                <span draggable = "true" ondragstart="drag(event, '{{divElementId}}')" class='text-warning' data-module="{{target.module}}" data-concept-id='{{type.conceptId}}' data-term='{{type.defaultTerm}}'>
                    {{#if_gr type.defaultTerm 0}}
                        {{substr type.defaultTerm 0}}
                    {{else}}
                        {{type.defaultTerm}}
                    {{/if_gr}}
                </span>&nbsp;&rArr;&nbsp;

                    {{#if_eq target.definitionStatus "Primitive"}}
                    <span id='{{@index}}{{../../../divElementId}}-treeicon-{{target.conceptId}}' draggable = "true" ondragstart="drag(event, '{{divElementId}}')" class='sct-primitive-concept-compact' data-module="{{target.module}}" data-concept-id='{{target.conceptId}}' data-term='{{target.defaultTerm}}'>
                    {{else}}
                    <span id='{{@index}}{{../../../divElementId}}-treeicon-{{target.conceptId}}' draggable = "true" ondragstart="drag(event, '{{divElementId}}')" class='sct-defined-concept-compact' data-module="{{target.module}}" data-concept-id='{{target.conceptId}}' data-term='{{target.defaultTerm}}'>
                    {{/if_eq}}
                    <button class='btn btn-link btn-xs treeButton' style='padding:2px'><i class='glyphicon glyphicon-chevron-right treeButton' data-ind="{{@index}}"></i></button>
                    {{target.defaultTerm}}
                </span>
                </li>
            {{/each}}
        </ul>
    {{/if_eq}}
{{else}}
    {{#if_eq inferredParents.length 0}}
        <span class='text-muted'>No parents</span>
    {{else}}
        <ul style='list-style-type: none; padding-left: 2px; padding-top: 0px;'>
            {{#each inferredParents}}
                <li data-concept-id='{{target.conceptId}}'>
                <span draggable = "true" ondragstart="drag(event, '{{divElementId}}')" class='text-warning' data-module="{{target.module}}" data-concept-id='{{type.conceptId}}' data-term='{{type.defaultTerm}}'>
                    {{type.defaultTerm}}
                </span>&nbsp;&rArr;&nbsp;
                    {{#if_eq target.definitionStatus "Primitive"}}
                    <span id='{{@index}}{{../../../divElementId}}-treeicon-{{target.conceptId}}' draggable = "true" ondragstart="drag(event, '{{divElementId}}')" class='sct-primitive-concept-compact' data-module="{{target.module}}" data-concept-id='{{target.conceptId}}' data-term='{{target.defaultTerm}}'>
                    {{else}}
                    <span id='{{@index}}{{../../../divElementId}}-treeicon-{{target.conceptId}}' draggable = "true" ondragstart="drag(event, '{{divElementId}}')" class='sct-defined-concept-compact' data-module="{{target.module}}" data-concept-id='{{target.conceptId}}' data-term='{{target.defaultTerm}}'>
                    {{/if_eq}}
                    <button class='btn btn-link btn-xs treeButton' style='padding:2px'><i class='glyphicon glyphicon-chevron-right treeButton' data-ind="{{@index}}"></i></button>
                    {{target.defaultTerm}}
                </span>
                </li>
            {{/each}}
        </ul>
    {{/if_eq}}
{{/if_eq}}