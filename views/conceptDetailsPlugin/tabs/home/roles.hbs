<div style='line-height: 100%;'>
    {{#if_eq options.selectedView "stated"}}
        {{#each statedRoles}}
            <br>
            {{#eqLastGroup groupId}}
                {{setLastGroup groupId}}
                {{lastColor getRandomColor}}
                &nbsp;&nbsp;&nbsp;<span style='background-color: {{lastColor "get"}}'>&nbsp;&nbsp;</span>
            {{else}}
                &nbsp;&nbsp;&nbsp;<span style='background-color: {{lastColor "get"}}'>&nbsp;&nbsp;</span>
            {{/eqLastGroup}}
            &nbsp;<span class='jqui-draggable sct-attribute-compact' data-concept-id='{{type.conceptId}}' data-term='{{type.defaultTerm}}'>
                {{removeSemtag type.defaultTerm}}</span>&nbsp;&rarr;&nbsp;

            {{#if_eq target.definitionStatus "Primitive"}}
                <span class='jqui-draggable sct-primitive-concept-compact' data-concept-id='{{target.conceptId}}' data-term='{{target.defaultTerm}}'>
            {{else}}
                <span class='jqui-draggable sct-defined-concept-compact' data-concept-id='{{target.conceptId}}' data-term='{{target.defaultTerm}}'>
            {{/if_eq}}
            {{removeSemtag target.defaultTerm}}
        </span><br>
        {{/each}}
        {{#if_eq statedRoles.length 0}}
            <span class='i18n text-muted' data-i18n-id='i18n_no_relationships'>No relationships</span>
        {{/if_eq}}
    {{else}}
        {{#each inferredRoles}}
            <br>
            {{#eqLastGroup groupId}}
                {{setLastGroup groupId}}
                {{lastColor getRandomColor}}
                &nbsp;&nbsp;&nbsp;<span style='background-color: {{lastColor "get"}}'>&nbsp;&nbsp;</span>
            {{else}}
                &nbsp;&nbsp;&nbsp;<span style='background-color: {{lastColor "get"}}'>&nbsp;&nbsp;</span>
            {{/eqLastGroup}}
            &nbsp;<span class='jqui-draggable sct-attribute-compact' data-concept-id='{{type.conceptId}}' data-term='{{type.defaultTerm}}'>
                {{removeSemtag type.defaultTerm}}</span>&nbsp;&rarr;&nbsp;
            <span class='jqui-draggable
                {{#if_eq target.definitionStatus "Primitive"}}
                     sct-primitive-concept-compact
                {{else}}
                     sct-defined-concept-compact
                {{/if_eq}}
                ' data-concept-id='{{target.conceptId}}' data-term='{{target.defaultTerm}}'>
                {{removeSemtag target.defaultTerm}}</span><br>
        {{/each}}
        {{#if_eq inferredRoles.length 0}}
            <span class='i18n text-muted' data-i18n-id='i18n_no_relationships'>No relationships</span>
        {{/if_eq}}
    {{/if_eq}}
</div>