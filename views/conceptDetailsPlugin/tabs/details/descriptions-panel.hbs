<table class='table table-bordered' id = '{{divElementId}}-descriptions-panel-table'>
    <thead><tr>
        <th><span class='i18n' data-i18n-id='i18n_term'>Term</span></th>
        {{#if options.showIds}}
            <th>SCTID</th>
        {{/if}}
        <th><span class='i18n' data-i18n-id='i18n_acceptability'>Acceptability</span>{{languageName}}</th>
        </tr>
    </thead>
    <tbody>

    {{#each allDescriptions}}
        {{#if ../options.displayInactiveDescriptions}}
            <tr class='{{#if_eq type.conceptId "900000000000003001"}} fsn-row{{else}} synonym-row{{/if_eq}}{{#if active}}{{else}} danger{{/if}}{{#if_eq effectiveTime ../../options.highlightByEffectiveTime}} highlightEffectiveTime{{/if_eq}}'>
                <td>
                    {{#if_eq type.conceptId "900000000000003001"}}
                        <span rel="tooltip-right" title="F">F</span>
                    {{else}}
                        {{#if_eq type.conceptId "900000000000013009"}}
                            <span rel="tooltip-right" title="S">S</span>
                        {{else}}
                            {{#if_eq type.conceptId "900000000000550004"}}
                                <span rel="tooltip-right" title="D">D</span>
                            {{/if_eq}}
                        {{/if_eq}}
                    {{/if_eq}}

                    {{#if_eq langMemberships "undefined"}}
                    {{else}}
                        {{#each langMemberships}}
                            {{#if_eq refset.conceptId ../../../../options.langRefset}}
                                {{#if_eq acceptability.conceptId "900000000000548007"}}
                                    <!--isPreferred = true;-->
                                    {{preferred true}}
                                {{else}}
                                    {{#if_eq acceptability.conceptId "900000000000549004"}}
                                        <!--isAcceptable = true;-->
                                        {{acceptable true}}
                                    {{/if_eq}}
                                {{/if_eq}}
                            {{/if_eq}}
                        {{/each}}
                    {{/if_eq}}
                    {{#preferred "get"}}
                        {{#if_eq type.conceptId "900000000000003001"}}
                            &nbsp;<span class="glyphicon glyphicon-star-empty" rel="tooltip-right" title="Preferred"></span>
                        {{else}}
                            &nbsp;<span class="glyphicon glyphicon-star" rel="tooltip-right" title="Preferred"></span>
                        {{/if_eq}}
                    {{else}}
                        {{#acceptable "get"}}
                            &nbsp;<span rel="tooltip-right" title="Acceptable">&#10004;</span></span>
                        {{else}}
                            &nbsp;&nbsp;&nbsp;
                        {{/acceptable}}
                    {{/preferred}}

                    &nbsp;&nbsp;&nbsp;{{term}}</td>
                {{#if ../options.showIds}}
                    <td>{{descriptionId}}</td>
                {{/if}}

                <td>
                    {{included false}}
                    {{#if_eq langMemberships "undefined"}}
                    {{else}}
                        {{#each langMemberships}}
                            {{#if_eq refset.conceptId ../../../../options.langRefset}}
                                {{#if_eq acceptability.conceptId "900000000000548007"}}
                                    <span class='i18n' data-i18n-id='i18n_preferred'>Preferred</span>
                                {{else}}
                                    <span class='i18n' data-i18n-id='i18n_acceptable'>Acceptable</span>
                                {{/if_eq}}
                                {{included true}}
                            {{/if_eq}}
                        {{/each}}
                    {{/if_eq}}

                    {{#included "get"}}
                    {{else}}
                        <span class='i18n' data-i18n-id='i18n_not_acceptable'>Not acceptable</span>
                    {{/included}}
                    <button type="button" class="btn btn-link unobtrusive-icon more-fields-button pull-right" data-container="body" data-toggle="popover" data-placement="left" data-content="
                    <table border='1'><tr><th style='padding: 3px;'>DescriptionId</th><th style='padding: 3px;'>Type</th><th style='padding: 3px;'>Language</th><th style='padding: 3px;'>Case Significance</th><th style='padding: 3px;'>Effective Time</th><th style='padding: 3px;'>ModuleId</th></tr>
                            <tr><td style='padding: 3px;'>{{descriptionId}}</td><td style='padding: 3px;'>{{removeSemtag type.defaultTerm}}</td><td style='padding: 3px;'>{{lang}}</td><td style='padding: 3px;'>{{removeSemtag ics.defaultTerm}}</td><td style='padding: 3px;'>{{effectiveTime}}</td><td style='padding: 3px;'>{{module}}</td></tr>
                    </table>
                    " data-html="true"><i class="glyphicon glyphicon-info-sign"></i></button>
                </td>
            </tr>
        {{else}}
            {{#if active}}
                <tr class='{{#if_eq type.conceptId "900000000000003001"}} fsn-row{{else}} synonym-row{{/if_eq}}{{#if active}}{{else}} danger{{/if}}{{#if_eq effectiveTime ../../../options.highlightByEffectiveTime}} highlightEffectiveTime{{/if_eq}}'>
                    <td>
                        {{#if_eq type.conceptId "900000000000003001"}}
                            <span rel="tooltip-right" title="F">F</span>
                        {{else}}
                            {{#if_eq type.conceptId "900000000000013009"}}
                                <span rel="tooltip-right" title="S">S</span>
                            {{else}}
                                {{#if_eq type.conceptId "900000000000550004"}}
                                    <span rel="tooltip-right" title="D">D</span>
                                {{/if_eq}}
                            {{/if_eq}}
                        {{/if_eq}}

                        {{#if_eq langMemberships "undefined"}}
                        {{else}}
                            {{#each langMemberships}}
                                {{#if_eq refset.conceptId ../../../../../options.langRefset}}
                                    {{#if_eq acceptability.conceptId "900000000000548007"}}
                                        <!--isPreferred = true;-->
                                        {{preferred true}}
                                    {{else}}
                                        {{#if_eq acceptability.conceptId "900000000000549004"}}
                                            <!--isAcceptable = true;-->
                                            {{acceptable true}}
                                        {{/if_eq}}
                                    {{/if_eq}}
                                {{/if_eq}}
                            {{/each}}
                        {{/if_eq}}
                        {{#preferred "get"}}
                            {{#if_eq type.conceptId "900000000000003001"}}
                                &nbsp;<span class="glyphicon glyphicon-star-empty" rel="tooltip-right" title="Preferred"></span>
                            {{else}}
                                &nbsp;<span class="glyphicon glyphicon-star" rel="tooltip-right" title="Preferred"></span>
                            {{/if_eq}}
                        {{else}}
                            {{#acceptable "get"}}
                                &nbsp;<span rel="tooltip-right" title="Acceptable">&#10004;</span></span>
                            {{else}}
                                &nbsp;&nbsp;&nbsp;
                            {{/acceptable}}
                        {{/preferred}}

                        &nbsp;&nbsp;&nbsp;{{term}}</td>
                    {{#if ../options.showIds}}
                        <td>{{descriptionId}}</td>
                    {{/if}}

                    <td>
                        {{included false}}
                        {{#if_eq langMemberships "undefined"}}
                        {{else}}
                            {{#each langMemberships}}
                                {{#if_eq refset.conceptId ../../../../../options.langRefset}}
                                    {{#if_eq acceptability.conceptId "900000000000548007"}}
                                        <span class='i18n' data-i18n-id='i18n_preferred'>Preferred</span>
                                    {{else}}
                                        <span class='i18n' data-i18n-id='i18n_acceptable'>Acceptable</span>
                                    {{/if_eq}}
                                    {{included true}}
                                {{/if_eq}}
                            {{/each}}
                        {{/if_eq}}

                        {{#included "get"}}
                        {{else}}
                            <span class='i18n' data-i18n-id='i18n_not_acceptable'>Not acceptable</span>
                        {{/included}}
                        <button type="button" class="btn btn-link unobtrusive-icon more-fields-button pull-right" data-container="body" data-toggle="popover" data-placement="left" data-content="
                        <table border='1'><tr><th style='padding: 3px;'>DescriptionId</th><th style='padding: 3px;'>Type</th><th style='padding: 3px;'>Language</th><th style='padding: 3px;'>Case Significance</th><th style='padding: 3px;'>Effective Time</th><th style='padding: 3px;'>ModuleId</th></tr>
                                <tr><td style='padding: 3px;'>{{descriptionId}}</td><td style='padding: 3px;'>{{removeSemtag type.defaultTerm}}</td><td style='padding: 3px;'>{{lang}}</td><td style='padding: 3px;'>{{removeSemtag ics.defaultTerm}}</td><td style='padding: 3px;'>{{effectiveTime}}</td><td style='padding: 3px;'>{{module}}</td></tr>
                        </table>
                        " data-html="true"><i class="glyphicon glyphicon-info-sign"></i></button>
                    </td>
                </tr>
            {{/if}}
        {{/if}}
        {{included false}}
        {{#each langMemberships}}
            {{#if_eq refset.conceptId ../../options.langRefset}}
                {{included true}}
            {{/if_eq}}
        {{/each}}
        {{#included "get"}}
            {{#if ../../options.hideNotAcceptable}}
                {{#if ../../../options.displayInactiveDescriptions}}
                    <!--descDetailsHtml = descDetailsHtml + row;-->
                {{/if}}
            {{else}}
                <!--descDetailsHtml = descDetailsHtml + row;-->
            {{/if}}
        {{else}}
            {{#if ../../options.displayInactiveDescriptions}}
                <!--descDetailsHtml = descDetailsHtml + row;-->
            {{/if}}
        {{/included}}
    {{/each}}
    </tbody>
</table>